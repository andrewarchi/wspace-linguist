{
  "$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
  "name": "Whitespace",
  "scopeName": "source.whitespace",
  "fileTypes": ["ws"],
  "patterns": [
    {
      "match": " ",
      "name": "markup.ignored.character.space.ws"
    },
    {
      "match": "\t",
      "name": "invalid.illegal.character.tab.ws"
    }
  ]
}
