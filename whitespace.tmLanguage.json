{
  "$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
  "name": "Whitespace",
  "scopeName": "source.ws",
  "fileTypes": ["ws"],
  "patterns": [
    {
      "name": "markup.ignored.character.space.ws",
      "match": " "
    },
    {
      "name": "invalid.illegal.character.tab.ws",
      "match": "\t"
    }
  ]
}
